{
    "manifest_version": 3,
    "name": "Fake News Checker",
    "version": "1.0",
    "description": "Check if an article is fake.",
    "permissions": ["activeTab"],
    "host_permissions": [
      "https://*.ngrok.io/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }